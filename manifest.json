{
  "manifest_version": 2,
  "name": "CookieSender",
  "description": "",
  "version": "0.0.1",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": {"scripts":["js/self.js"]},
  "permissions": [ "*://*/*", "webRequest", "webRequestBlocking", "tabs", "storage", "cookies", "history" ]
}